    <insert id="addPicture" parameterType="hs.bean.PictureLocal" useGeneratedKeys="true" keyColumn="id"
    keyProperty="id">
    insert into picturelocation(location, saveTime,batchNo,cameraId) values
    (#{pictureLocal.location},#{pictureLocal.saveTime ,typeHandler=hs.dao.typeHandler.InstantHandler},#{pictureLocal.batchNo},#{pictureLocal.cameraId})
    </insert>

    <select id="getPictures" resultType="hs.bean.PictureLocal">
      select * from picturelocation
      where saveTime &gt;=#{begin,typeHandler=hs.dao.typeHandler.InstantHandler}
      and saveTime &lt;=#{start_time,typeHandler=hs.dao.typeHandler.InstantHandler}
    </select>



